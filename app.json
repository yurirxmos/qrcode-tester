{
    "expo": {
        "name": "qrcode-tester",
        "slug": "qrcode-tester",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#000000"
        },
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/icon.png",
                "backgroundColor": "#ffffff"
            },
            "permissions": ["CAMERA", "RECORD_AUDIO"],
            "package": "com.yurirxmos.qrcodetester"
        },
        "web": {
            "favicon": "./assets/icon.png"
        },
        "extra": {
            "eas": {
                "projectId": "f18582ef-8eb5-44d3-bf01-0276dae64091"
            }
        },
        "owner": "yurirxmos",
        "runtimeVersion": {
            "policy": "appVersion"
        },
        "updates": {
            "enabled": true,
            "fallbackToCacheTimeout": 0,
            "checkAutomatically": "ON_LOAD",
            "url": "https://u.expo.dev/f18582ef-8eb5-44d3-bf01-0276dae64091"
        }
    }
}
